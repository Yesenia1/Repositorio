    <div class="display-label">CELULAR</div>
    <div class="display-field">@Model.CELULAR</div>

    <div class="display-label">EMPRESA</div>
    <div class="display-field">@Model.EMPRESA</div>

    <div class="display-label">EMAIL</div>
    <div class="display-field">@Model.EMAIL</div>

    <div class="display-label">DIRECCION</div>
    <div class="display-field">@Model.DIRECCION</div>

    <div class="display-label">EXPECTATIVA</div>
    <div class="display-field">@Model.EXPECTATIVA</div>

    <div class="display-label">INTERES</div>
    <div class="display-field">@Html.DisplayTextFor(_ => Model.INTERES).ToString()</div>

    <div class="display-label">PODER</div>
    <div class="display-field">@Html.DisplayTextFor(_ => Model.PODER).ToString()</div>

    <div class="display-label">PROYECTO</div>
